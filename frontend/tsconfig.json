{
  "compilerOptions": {
    "target": "es5",
    "plugins": [
      {
        "name": "typescript-styled-plugin"
      }
    ],
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": false,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "noImplicitAny": false,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "downlevelIteration": true,
    "noEmit": true,
    "jsx": "preserve"
  },
  "include": [
    "src",
  ],
  "exclude": [
    "node_modules",
    "**/node_modules/*",
    "**/staticresources/*"
  ]
}
